{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true,
    "**/*.gen.d.ts": true,
    "out/**": true,
    "dist/**": true,
    "build/**": true,
    "node_modules/**": true,
    ".tanstack/**": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true,
    "**/*.gen.d.ts": true,
    "out/**": true,
    "dist/**": true,
    "build/**": true,
    "node_modules/**": true,
    ".tanstack/**": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true,
    "**/*.gen.d.ts": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "out/**": true,
    "dist/**": true,
    "build/**": true,
    "node_modules/**": true,
    ".tanstack/**": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit",
    "source.sortImports": "explicit"
  }
}
