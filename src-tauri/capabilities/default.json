{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPLOCALDATA/**"
        }
      ]
    },
    "fs:allow-create",
    "fs:allow-exists",
    "fs:allow-write",
    "fs:allow-mkdir",
    "sql:default",
    "sql:allow-execute"
  ]
}
