{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/src-client/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*"],
  "modes": {
    "window": {
      "title": "Sapphire",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/src-client/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "Sapphire",
    "resourcesPath": "/src-client/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.1.0",
    "clientVersion": "6.1.0",
    "frontendLibrary": {
      "patchFile": "/src-client/index.html",
      "devUrl": "http://localhost:3000",
      "projectPath": "/src-client/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}
